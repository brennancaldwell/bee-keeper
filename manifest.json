{
    "name": "Bee Keeper",
    "version": "1.0.0",
    "description": "NYT Spelling Bee Helper",
    "manifest_version": 3,
    "author": "Brennan Caldwell",
    "action":{
        "default_popup": "index.html",
        "default_title": "Bee Keeper",
        "default_icon": {
          "16": "/images/beehive-16.png",
          "32": "/images/beehive-32.png",
          "48": "/images/beehive-48.png",
          "128": "/images/beehive-128.png"
        }
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "icons": {
      "16": "/images/beehive-16.png",
      "32": "/images/beehive-32.png",
      "48": "/images/beehive-48.png",
      "128": "/images/beehive-128.png"
    }
}